{
    "builds":[
        {
        "src": "AAPI-AI-DJANGO-CHATBOT/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "AAPI-AI-DJANGO-CHATBOT/wsgi.py"
        }
    ]
}